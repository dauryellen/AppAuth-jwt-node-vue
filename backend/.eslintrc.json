{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "quotes": ["error", "double"],
    "comma-dangle": ["error", "never"],
    "linebreak-style": ["error", "windows"],
    "no-console": "off"
  }
}
